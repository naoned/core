{
    "name": "onyx/core",
    "type": "library",
    "license": "MIT",
    "description": "Onyx library for Onyx skeleton",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/naoned/silex.git"
        }
    ],
    "require": {
        "php": "^8.1",
        "silex/silex": "dev-val-php8.1@dev",
        "puzzle/configuration": "^4.0.2",
        "twig/twig": "^2.12|^3.0",
        "symfony/console" : "^5.0",
        "symfony/twig-bridge" : "~3.1",
        "puzzle/pieces": "dev-clb-sf5@dev",
        "monolog/monolog": "~1.22",
        "psr/log": "~1.0",
        "ramsey/uuid": "^4.0"
    },
    "require-dev" : {
        "phpunit/phpunit" : "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Onyx\\": "src/"
        }
    }
}
